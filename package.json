{
  "name": "procx",
  "version": "1.0.0",
  "description": "Modern cross-platform process and port management CLI & API for developers - Procx",
  "main": "dist/api/index.js",
  "types": "dist/api/index.d.ts",
  "bin": {
    "procx": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && node scripts/make-executable.js",
    "build:all": "node scripts/build-all.js",
    "build:platform": "node scripts/build-platform.js",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "dist": "npm run build:all && node scripts/create-dist.js",
    "validate": "node scripts/validate-build.js",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepublishOnly": "npm run clean && npm run build:all && npm run build:platform && npm run lint && npm run format:check && npm run validate",
    "prepack": "npm run build",
    "postinstall": "echo \"Procx installed successfully! Run 'procx --help' to get started.\""
  },
  "keywords": [
    "procx",
    "process",
    "processes",
    "process management",
    "process manager",
    "process monitor",
    "task manager",
    "system monitor",
    "system processes",
    "port",
    "ports",
    "port management",
    "port scanner",
    "port finder",
    "free port",
    "find free port",
    "port conflict",
    "kill port",
    "kill process",
    "signal",
    "ps alternative",
    "netstat alternative",
    "lsof alternative",
    "cli",
    "command-line",
    "terminal",
    "devtools",
    "developer tools",
    "devops",
    "node",
    "nodejs",
    "node.js",
    "typescript",
    "cross-platform",
    "macos",
    "linux",
    "windows",
    "server",
    "backend",
    "debugging",
    "performance",
    "resource usage",
    "open source",
    "opensource",
    "mit license"
  ],
  "author": {
    "name": "Anurag Vikram Singh",
    "email": "anuragvs.official@gmail.com",
    "url": "https://github.com/AnuragVikramSingh"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnuragVikramSingh/procx.git"
  },
  "bugs": {
    "url": "https://github.com/AnuragVikramSingh/procx/issues"
  },
  "homepage": "https://github.com/AnuragVikramSingh/procx#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=7.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "preferGlobal": true,
  "dependencies": {
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/AnuragVikramSingh"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}